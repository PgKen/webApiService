                <div class="ad-boxArticle">
                    <div class="ad-form">
                        <form action="/adminLogin" method="post">
                            <ul>
                                <li>
                                    <label for="">User</label>
                                    <input type="text" name="name">
                                </li>
                                <li>
                                    <label for="">Password</label>
                                    <input type="password" name="password">
                                </li>
                                <li><button>Login</button></li>
                            </ul>
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>
